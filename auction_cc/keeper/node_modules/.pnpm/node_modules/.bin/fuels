#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vaibhavgoyal/Desktop/web3/cross-chain-auction/auction_cc/keeper/node_modules/.pnpm/fuels@0.101.1_vitest@3.0.9_@types+node@20.19.21_/node_modules/fuels/node_modules:/Users/vaibhavgoyal/Desktop/web3/cross-chain-auction/auction_cc/keeper/node_modules/.pnpm/fuels@0.101.1_vitest@3.0.9_@types+node@20.19.21_/node_modules:/Users/vaibhavgoyal/Desktop/web3/cross-chain-auction/auction_cc/keeper/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vaibhavgoyal/Desktop/web3/cross-chain-auction/auction_cc/keeper/node_modules/.pnpm/fuels@0.101.1_vitest@3.0.9_@types+node@20.19.21_/node_modules/fuels/node_modules:/Users/vaibhavgoyal/Desktop/web3/cross-chain-auction/auction_cc/keeper/node_modules/.pnpm/fuels@0.101.1_vitest@3.0.9_@types+node@20.19.21_/node_modules:/Users/vaibhavgoyal/Desktop/web3/cross-chain-auction/auction_cc/keeper/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fuels/fuels.js" "$@"
else
  exec node  "$basedir/../fuels/fuels.js" "$@"
fi
