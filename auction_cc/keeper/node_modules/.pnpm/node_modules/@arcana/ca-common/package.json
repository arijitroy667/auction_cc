{
  "name": "@arcana/ca-common",
  "version": "1.0.1-alpha.6",
  "description": "common utilities for CA",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./fuelcontracts": {
      "types": "./dist/types/fuelcontracts/index.d.ts",
      "require": "./dist/cjs/fuelcontracts/index.js",
      "import": "./dist/esm/fuelcontracts/index.js"
    }
  },
  "scripts": {
    "build": "run-s clean build:esm build:cjs",
    "build:esm": "tsc --project tsconfig.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint",
    "clean": "rimraf dist/",
    "prepublishOnly": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@types/node": "^22.16.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-proto": "^2.7.5",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@cosmjs/proto-signing": "^0.34.0",
    "@cosmjs/stargate": "^0.34.0",
    "axios": "^1.10.0",
    "decimal.js": "^10.6.0",
    "fuels": "0.101.1",
    "long": "^5.3.2",
    "viem": "^2.31.7",
    "msgpackr": "^1.11.4"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.6.0",
    "@improbable-eng/grpc-web": "^0.15.0",
    "browser-headers": "^0.4.1",
    "es-toolkit": "^1.39.7",
    "tslib": "^2.8.1"
  }
}
